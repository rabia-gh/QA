pipeline {
  agent any
  stages {
    stage('Checkout Scm') {
      steps {
        git(credentialsId: 'github', url: 'https://github.com/rabia-gh/QA.git')
      }
    }

    stage('No Converter-0') {
      steps {
        echo 'No converter for Builder: com.katalon.jenkins.plugin.ExecuteKatalonStudioTask'
      }
    }

  }
  post {
    always {
      step($class: 'JUnitResultArchiver', testResults: 'report/**/JUnit_Report.xml')
    }

  }
}

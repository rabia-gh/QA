pipeline {
 agent any
 stages {
  stage('SCM') {
   steps {
    checkout scm
   }
  }
  stage('Dev stage') {
   parallel {
     stage('Dev Backend ') {
     steps {
     job:backend
     }
     }
     stage('Dev Frontend ')
          steps {
     job:frontend
     }
     }
     }
     stage('Test stage') {
   parallel {
     stage('Test Backend ') {
     steps {
     job:backend
     }
     }
     stage('Test Frontend ')
          steps {
     job:frontend
     }
     }
     }
   }
   }
   
